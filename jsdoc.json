{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.(js(doc|x)?|mjs)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",
        "destination": "doc",
        "recurse": true,
        "readme": "README.md"
    }
}
